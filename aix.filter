#!/bin/ksh
#
# Notes; This file must have permissions of 755 and be owned by bin:printq
# Change printer value to the name of your standard queue
umask 000
	file=$1
	printer="lp_sp5300dn"
	filename=`echo $1|awk 'BEGIN {FS="/";} {print $NF;}'`
	
	/usr/local/bin/myfilters/rgwapf < $file > /tmp/$filename 
	
	lp -cd $printer /tmp/$filename

	#rm /tmp/$filename
		
exit $?
# 
